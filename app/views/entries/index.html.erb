<h1>My Rails Girls Diary</h1>
<div>
<p><%= link_to("New entry", new_entry_path) %></p>
</div>
<div>
  <h2>My diary</h2>
    <% for entry in @entries %>
    <h2><%= link_to(entry.title, entry_path(entry)) %></h2>
      <p> <%= entry.date %> </p>
      <p> <%= entry.contents %> </p>
    <% end %>



</div>
